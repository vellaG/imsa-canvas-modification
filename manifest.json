{
    "name": "Amolsoft",
    "description": "A simple dark reskin for canvas",
    "version": "1.2",
    "manifest_version": 3,
    "content_scripts":[
        {
            "matches": ["https://*.instructure.com/*"],
            "js": ["rootcolors.js"]
        
        },
        {
            "matches": ["https://*.instructure.com/*"],
            "js": ["/js/dashboard.js"]
        
        },
        {
            "matches": ["https://imsa.instructure.com/*"],
            "js": ["/js/imsalogochange.js"]
        
        }],
    "permissions": ["storage"],
    "web_accessible_resources": [
        {
          "resources": [ "/css/*"],
          "matches": [ "https://*.instructure.com/*" ]
        }
      ]
  }